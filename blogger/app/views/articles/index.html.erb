<h1>All Articles</h>

  <ul id="articles">
      <% @articles.each do |article| %>
        <li>
            <%= link_to article.title, article_path(article), id: "article_#{article.id}" %>
        </li>
      <% end %>
  </ul>

<%= link_to "Create a New Article", new_article_path, class: 'new_article' %>
